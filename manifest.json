{
    "name": "Example Name",
    "version": "1.0",
    "description": "This is a description.",
    "permissions": 
    [
        "declarativeContent", 
        "activeTabs",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false  
    },
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "32": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        }
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "manifest_version": 2
}